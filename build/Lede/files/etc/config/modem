
config global 'global'
	option enable_dial '1'
	option manual_configuration '0'
	option network_interface_alias '1'
	option modem_number '1'

config modem-device 'modem0'
	option data_interface 'pcie'
	option path '/sys/devices/pci0000:00/0000:00:14.0/0000:05:00.0'
	option network 'rmnet_mhi0'
	option network_interface 'rmnet_mhi0.1'
	list ports '/dev/mhi_DIAG'
	list ports '/dev/mhi_QMI0'
	list ports '/dev/mhi_BHI'
	list ports '/dev/mhi_LOOPBACK'
	list ports '/dev/mhi_DUN'
	option at_port '/dev/mhi_DUN'
	option define_connect '1'
	option name 'rm500q-gl'
	option manufacturer 'quectel'
	option platform 'qualcomm'
	list modes 'qmi'
	list modes 'gobinet'
	list modes 'mbim'

config dial-config '3b5c6310c2f34ea1b8bcf3f8d3602c25'
	option network_bridge '0'
	option enable '1'
	option network 'rmnet_mhi0'
	option pdp_type 'ipv4'
	option auth 'none'
	option id '3b5c6310c2f34ea1b8bcf3f8d3602c25'

